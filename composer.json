{
    "name": "erenilhan/ss2i-openstreetmap-custom",
    "description": "Custom Openstreet Map",
    "autoload": {
        "psr-4": {
            "Erenilhan\\Ss2iOpenstreetmapCustom\\": "src/"
        }
    },
    "authors": [
        {
            "name": "erenilhan",
            "email": "erenilhan@ss2i-digital.fr"
        }
    ],

    "require": {
        "php": "^8.1",
        "filament/filament": "^3",
        "filament/forms": "^3",
        "illuminate/contracts": "^10.0",
        "matanyadaev/laravel-eloquent-spatial": "3.2.2",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Erenilhan\\FilamentOpenStreetMap\\FilamentOpenStreetMapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
